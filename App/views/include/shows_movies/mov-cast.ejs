<div id="cast-container">
    <h2 id="cast-header">Cast</h2>
    <div id="ep-items">
      <% casts.forEach(cast=>{ %>
        <a href="/actor/<%= cast.actor_name  %>" style="text-decoration: none;color:inherit">
          <div class="ep-item">
            <% if(cast.image == '') {%>
              <img src="https://imgs.search.brave.com/nRcYZiM3fzEBsYg8UplCApvFVgn3gm4ejAghRxvXXqc/rs:fit:800:1000:1/g:" alt="no image">
          <% } else { %>
              <img src="<%= cast.image %>" alt="<%= cast.character_name %>">
              <% } %>
              <h3 class="name"><%= cast.actor_name  %></h3>
              <h3 class="character"><%= cast.character_name %></h3>
             </div>
        </a>
        <% }) %>
        
    </div>
</div>